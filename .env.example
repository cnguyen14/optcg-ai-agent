# Database
DATABASE_URL=postgresql://optcg:dev_password@localhost:5432/optcg_dev

# Redis
REDIS_URL=redis://localhost:6379

# Qdrant
QDRANT_URL=http://localhost:6333

# AI Provider API Keys (configure at least one)
# Get Anthropic key: https://console.anthropic.com/
ANTHROPIC_API_KEY=your_anthropic_key_here

# Get OpenAI key: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_key_here

# Get OpenRouter key: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_key_here

# Get Google AI key: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your_google_key_here

# Get Kimi key: https://platform.moonshot.cn/console/api-keys
KIMI_API_KEY=your_kimi_key_here

# Default AI provider to use (anthropic, openai, openrouter, gemini, kimi)
DEFAULT_AI_PROVIDER=anthropic

# App
SECRET_KEY=your_secret_key_here
ENVIRONMENT=development

# Frontend
NEXT_PUBLIC_API_URL=http://localhost:8000
